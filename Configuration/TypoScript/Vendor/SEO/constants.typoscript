/*
  --------------------------
  | T3v Page SEO Constants |
  --------------------------
*/

/*
  === Imports ===
*/

@import 'EXT:seo/Configuration/TypoScript/XmlSitemap/constants.typoscript'

/*
  === Plugin Constants ===
*/

plugin {
  tx_seo {
    settings {
      xmlSitemap {
        sitemaps {
          pages {
            excludedDoktypes = 3, 4, 6, 7, 199, 254, 255
            additionalWhere = {#no_index} = 0 AND {#canonical_link} = ''
          }
        }
      }
    }
  }
}
